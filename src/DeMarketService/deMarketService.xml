<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
	<processing-directives>
		<overwrite-existing-data>true</overwrite-existing-data>
	</processing-directives>
	<schedule>
		<job>
			<name>CustomerRebateService</name>
			<group>GroupBusinessServiceApis</group>
			<description>生成客户邀请佣金</description>
			<job-type>deMarketService.jobs.CustomerRebateService, deMarketService</job-type>
			<durable>true</durable>
			<recover>false</recover>
		</job>
		<trigger>
			<cron>
				<name>TriggerCustomerRebateService</name>
				<group>GroupTriggerBusinessService</group>
				<job-name>CustomerRebateService</job-name>
				<job-group>GroupBusinessServiceApis</job-group>
				<start-time>2019-03-13T15:15:00.0Z</start-time>
				<!--每天凌晨执行一次-->
				<cron-expression>0 0 0 * * ?</cron-expression>
			</cron>
		</trigger>
		<job>
			<name>ReadNFTService</name>
			<group>GroupBusinessService</group>
			<description>生成客户邀请佣金</description>
			<job-type>deMarketService.jobs.ReadNFTService, deMarketService</job-type>
			<durable>true</durable>
			<recover>false</recover>
		</job>
		<trigger>
			<cron>
				<name>TriggerReadNFTService</name>
				<group>GroupTriggerBusinessService</group>
				<job-name>ReadNFTService</job-name>
				<job-group>GroupBusinessService</job-group>
				<start-time>2019-03-13T15:15:00.0Z</start-time>
				<!--每5分钟-->
				<cron-expression>0 0/5 * * * ?</cron-expression>
			</cron>
		</trigger>
	</schedule>
</job-scheduling-data>